@import url('https://fonts.googleapis.com/css2?family=Roboto:wght@100;300;400;500;700;900&display=swap');
#header .navbar
{
	width:100%;
	padding:6px 0px 4px 15px;
  opacity: 0.8;
  background-color: #525252;
}
#header .navbar .navbar-brand 
{
	width: 73px;
  height: 54px;
	padding-top: 0rem; 
    padding-bottom: 0rem; 
    margin-right: 1rem;
}
#header .navbar .navbar-nav .nav-item .nav-link
{
  font-family: Roboto;
  font-size: 17px;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: 1.41;
  letter-spacing: normal;
  color: #fff;
}
#header .navbar .navbar-nav .book-now .nav-link
{
  height: 40px;
  padding: 12px 19px 15px 18px;
  border-radius: 20px;
  border: solid 1px rgba(255, 255, 255, 0.5);
  background-color: #29626d;
}
/* search section start*/
#search img
{
	width:100%;
	object-fit:contain;
}
/* search section end*/
/* faq section start*/
#faq 
{
	margin-right: auto;
    margin-left: auto;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: center;
	
}
#faq .title
{
	width: 80px;
  height: 30px;
  margin-top: 40px;
  margin-bottom:14px;
  font-family: Roboto;
  font-size: 36px;
  font-weight: 500;
  font-stretch: normal;
  font-style: normal;
  line-height: 0.67;
  letter-spacing: normal;
  color: #4f4f4f;
}
#faq .star
{
	display:flex;
}
#faq .star .line1
{
	width: 62px;
  height: 1px;
  margin:26px 9px 49px auto;
  background-color: #ccc;
}
#faq .star img
{
	 width: 24px;
  height: 25px;
  margin: 14px 8px 37px 9px;
  color: #1d7a8c;
}
#faq .star .line2
{
	width: 62px;
  height: 1px;
  margin:26px auto 49px 8px;
  background-color: #ccc;
}
#faq .text
{
	max-width: 491px;
  height: 44px;
  font-family: Roboto;
  font-size: 18px;
  font-weight: normal;
  font-stretch: normal;
  font-style: normal;
  line-height: 1.44;
  letter-spacing: normal;
  text-align: center;
  color: #646464;
}
#faq .row 
{
	margin-top:36px;
}
#faq .for-customer
{
  height: 66px;
  padding: 25px 149px 24px;
  background-color: #1d7a8c;
}
#faq .for-provider
{
  height: 66px;
  padding: 25px 149px 24px;
  background-color: #f6f6f6;
}
#faq .text-content1
{
	display:flex;
	margin-top:45px;
}
#faq .text-content1 img
{
	 width: 22px;
	height: 22px;
	object-fit: contain;
	margin-left:16px;
}
#faq .text-content1 .text1
{
  height: 15px;
  font-family: Roboto;
  font-size: 16px;
  font-weight: bold;
  font-stretch: normal;
  font-style: normal;
  letter-spacing: normal;
  text-align: left;
  color: #646464;
}
/* faq section end*/